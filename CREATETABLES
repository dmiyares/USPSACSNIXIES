headers 


CREATE TABLE `USPS`.`HEADERS`  (
`fileID` int(11) NOT NULL,
RecordType						 char(1),        
FileVersion						 varchar(2),        
CustomerID             varchar(6),        
CreateDate             varchar(8),        
ShipmentNumber         varchar(10),       
TotalAcsRecordCount    varchar(9),        
TotalCoaCount          varchar(9),        
TotalNixieCount        varchar(9),        
TrdRecordCount         varchar(9),        
TrdAcsFeeAmount        varchar(11),       
TrdCoaCount            varchar(9),        
TrdCoaAcsFeeAmount     varchar(11),       
TrdNixieCount          varchar(9),        
TrdNixieAcsFeeAmount   varchar(11),       
OcdRecordCount         varchar(9),        
OcdAcsFeeAmount        varchar(11),       
OcdCoaCount            varchar(9),        
OcdCoaAcsFeeAmount     varchar(11),       
OcdNixieCount          varchar(9),        
OcdNixieAcsFeeAmount   varchar(11),       
FsRecordCount          varchar(9),        
FsAcsFeeAmount         varchar(11),       
FsCoaCount             varchar(9),        
FsCoaAcsFeeAmount      varchar(11),       
FsNixieCount           varchar(9),        
FsNixieAcsFeeAmount    varchar(11),       
ImpbRecordCount        varchar(9),        
ImpbAcsFeeAmount       varchar(11),       
ImpbCoaCount           varchar(9),        
ImpbCoaAcsFeeAmount    varchar(11),       
ImpbNixieCount         varchar(9),        
ImpbNixieAcsFeeAmount  varchar(11),       
Filler                 varchar(405),      
EndMarker              char(1),  
  PRIMARY KEY (`fileID`)      
);                      




nixies  



CREATE TABLE `USPS`.`nixies`  (
id int(11) NOT NULL,
`fileID` int(11) NOT NULL,
RecordType                  char(1),
FileVersion                 varchar(2),
SequenceNumber              varchar(8),
AcsMailerId                 varchar(9),
KeylineSequenceSerialNumber varchar(16),
Filler1                     varchar(9),
DeliverabilityCode          char(1),
UspsSiteID                  varchar(3),
Filler2                     varchar(166),
OldZIPCode                  varchar(5),
Filler3                     varchar(218),
FeeNotification             char(1),
NotificationType            char(1),
IntelligentMailbarcode      varchar(31),  
IntelligentPackagebarcode   varchar(35),  
IdTag                       varchar(16),  
HardcopyToElectronicFlag    char(1),  
TypeOfAcs                   char(1),  
FulfillmentDate             varchar(8),  
ProcessingType              char(1),  
CaptureType                 char(1),  
Filler4                     varchar(8),  
ShapeOfMail                 char(1),  
MailActionCode              char(1),  
NixieFlag                   char(1),  
ProductCode1                varchar(6),  
ProductCodeFee1             varchar(6),  
ProductCode2                varchar(6),  
ProductCodeFee2             varchar(6),  
ProductCode3                varchar(6),  
ProductCodeFee3             varchar(6),  
ProductCode4                varchar(6),  
ProductCodeFee4             varchar(6),  
ProductCode5                varchar(6),  
ProductCodeFee5             varchar(6),  
ProductCode6                varchar(6),  
ProductCodeFee6             varchar(6),
Filler5                     varchar(81),
EndMarker                   varchar(6),
  PRIMARY KEY (`id`)      
);                                                  



coa

CREATE TABLE `USPS`.`coa`  (
ID int(11) NOT NULL,
`fileID` int(11) NOT NULL,
RecordType                     varchar(1),
FileVersion                    varchar(2),
SequenceNumber                 varchar(8),
AcsMailerId                    varchar(9),
KeylineSequenceSerialNumber    varchar(16),
MoveEffectiveDate              varchar(8),
MoveType                       varchar(1),
DeliverabilityCode             varchar(1),
UspsSiteID                     varchar(3),
LastName                       varchar(20),
FirstName                      varchar(15),
Prefix                         varchar(6),
Suffix                         varchar(6),
OldAddressType                 varchar(1),
OldUrb                         varchar(28),
OldPrimaryNumber               varchar(10),
OldPreDirectional              varchar(2),
OldStreetName                  varchar(28),
OldSuffix                      varchar(4),
OldPostDirectional             varchar(2),
OldUnitDesignator              varchar(4),
OldSecondaryNumber             varchar(10),
OldCity                        varchar(28),
OldStateAbbreviation           varchar(2),
OldZipCode                     varchar(5),
NewAddressType                 varchar(1),
NewPmb                         varchar(8),
NewUrb                         varchar(28),
NewPrimaryNumber               varchar(10),
NewPreDirectional              varchar(2),
NewStreetName                  varchar(28),
NewSuffix                      varchar(4),
NewPostDirectional             varchar(2),
NewUnitDesignator              varchar(4),
NewSecondaryNumber             varchar(10),
NewCity                        varchar(28),
NewStateAbbreviation           varchar(2),
NewZipCode                     varchar(5),
Hyphen                         varchar(1),
NewPlus4Code                   varchar(4),
NewDeliveryPoint               varchar(2),
NewAbbreviatedCityName         varchar(13),
NewAddressLabel                varchar(66),
FeeNotification                varchar(1),
NotificationType               varchar(1),
IntelligentMailBarcode         varchar(31),
IntelligentMailPackageBarcode  varchar(35),
IdTag                          varchar(16),
HardcopyToElectronicFlag       varchar(1),
TypeOfAcs                      varchar(1),
FulfillmentDate                varchar(8),
ProcessingType                 varchar(1),
CaptureType                    varchar(1),
MadeAvailableDate              varchar(8),
ShapeOfMail                    varchar(1),
MailActionCode                 varchar(1),
NixieFlag                      varchar(6),
ProductCode1                   varchar(6),
ProductCodeFee1                varchar(6),
ProductCode2                   varchar(6),
ProductCodeFee2                varchar(6),
ProductCode3                   varchar(6),
ProductCodeFee3                varchar(6),
ProductCode4                   varchar(6),
ProductCodeFee4                varchar(6),
ProductCode5                   varchar(6),
ProductCodeFee5                varchar(6),
ProductCode6                   varchar(6),
ProductCodeFee6                varchar(6),
DeliveryPointValidation				varchar(1),
Filler                         varchar(81),
EndMarker                      varchar(1),

  PRIMARY KEY (`id`)      
);          